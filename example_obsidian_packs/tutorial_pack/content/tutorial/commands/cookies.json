{
    "name": "cookies",
    "description": "Allow to give a number of cookies at yourself",
    "op_level": 2,
    "arguments": {
        "amount" : {
            "type": "integer",
            "execute": [
                "give @s cookie"
            ]
        },
        "user": {
            "type": "player",
            "execute": [
                "give {user} cookie"
            ],
            "arguments": {
                "amount": {
                    "type": "integer",
                    "execute": [
                        "give {user} cookie {amount}"
                    ]
                }
            }
        }
    }
}